pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying app'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing code'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
